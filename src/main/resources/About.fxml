<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.*?>
<AnchorPane xmlns:fx="http://javafx.com/fxml/1" prefHeight="416.0" prefWidth="403.0"
            xmlns="http://javafx.com/javafx/10.0.2-internal" fx:controller="View.AboutController">
    <BorderPane prefHeight="416.0" prefWidth="403.0">
        <bottom>
            <Button fx:id="close" mnemonicParsing="false" onAction="#close" text="Close" BorderPane.alignment="CENTER">
                <BorderPane.margin>
                    <Insets bottom="10.0"/>
                </BorderPane.margin>
            </Button>
        </bottom>
        <center>
            <ScrollPane prefWidth="403.0" BorderPane.alignment="CENTER">
                <prefHeight>300.0</prefHeight>
                <Label fx:id="Itext" text="About">
                    <padding>
                        <Insets left="10.0"/>
                    </padding>
                </Label>
            </ScrollPane>
        </center>
    </BorderPane>
</AnchorPane>
